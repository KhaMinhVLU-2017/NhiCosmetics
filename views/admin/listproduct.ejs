<div id="page-wrapper">
    <div class="row">
         <!--  page header -->
        <div class="col-lg-12">
            <h1 class="page-header">List Product</h1>
        </div>
        <!--end  page header -->
    </div>
    <table class="table table-hover">
        <tr>
            <th>Name</th>
            <th>Title</th>
            <th>Type</th>
            <th>Source</th>
            <th>Description</th>
            <th>Content</th>
            <th>Images</th>
            <th>Price</th>
            <th>Unit</th>
            <th>Note</th>
        </tr>
        <% entities.forEach(function(item){ %>
            <tr>
                <td><%= item.name %></td>
                <td><%= item.title %></td>
                <td><%= item.idType %></td>
                <td><%= item.source %></td>
                <td><%= item.description %></td>
                <td><%= item.content %></td>
                <td><img src="<%= item.Images[0].imgSrc %>" class="img-responsive" alt="<%= item.name %>"></td>
                <td><%= item.price %></td>
                <td><%= item.unitPrice %></td>
                <td><a href='/admin/delete/<%= item.id %>'>Delete</td>
            </tr>
        <% })%>
    </table>
</div>